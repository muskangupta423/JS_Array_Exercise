    <script>
        color = ["Blue ", "Green", "Red", "Orange", "Violet", "Indigo", "Yellow "];
        o = ["th","st","nd","rd"]
        for(let x=0;x<color.length;x++)
        {
            if(x==0)
                console.log(x+1+o[1]+" choice is "+color[x]+"</br>")
            else if(x==1)
                console.log(x+1+o[2]+" choice is "+color[x]+"</br>")
            else if(x==2)
                console.log(x+1+o[3]+" choice is "+color[x]+"</br>")
            else
                console.log(x+1+o[0]+" choice is "+color[x]+"</br>")
        }

    </script>


