    <p id="demo"></p>
    <script>
        var arr=[10,20,10,40,50,60,70], target=50
        for(let i=0;i<arr.length;i++)
        {
            for(let j=i+1;j<arr.length;j++)
            {
                if(arr[i]+arr[j]==target)
                {
                    console.log("Pair of elements are \n")
                    console.log(i+" "+j+"\n")
                }
            }
        }

    </script>
