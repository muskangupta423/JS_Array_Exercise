<body>
    <script>
        var arr = [1,0,2,3,4,0,5,2,7,2,0,2];
        var res=[]
        for(let x=0;x<arr.length;x++)
        {
            for(let y=x+1;y<arr.length;y++)
            {
                if(arr[y]==arr[x])
                {
                    res.push(arr[y])
                    break
                }
            }
        }
        var result=new Set(res);
        console.log(result)
    </script>
</body>
