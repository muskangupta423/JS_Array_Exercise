<body>
    <script>
        function first(arr,n){
            if(n==null)
                return arr[arr.length-1]
            else if(n<0)
                return []
            else if(n<arr.length)
                return arr.slice(arr.length-n,arr.length)
            return arr.slice()

        }
        console.log(first([7, 9, 0, -2]));
        console.log(first([],3));
        console.log(first([7, 9, 0, -2],3));
        console.log(first([7, 9, 0, -2],6));
        console.log(first([7, 9, 0, -2],-3));
    </script>


</body>
</html>
